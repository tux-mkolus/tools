
resource "fortios_firewall_addrgrp" "{{ resource_name }}" {
  allow_routing = "{{ allow_routing | default('disable') }}"
  color         = {{ color | default(0) }}
  name          = "{{ name | default(resource_name) }}"

{% for member in members %}
  member {
    name = fortios_firewall_address.{{ member }}.name
  }
{% endfor %}

}